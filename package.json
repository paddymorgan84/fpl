  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2"
  },
  "scripts": {
    "toc": "doctoc --title \"## Table of Contents\" .",
    "spell-check": "mdspell -arn --en-gb \"{infrastructure,scripts}/**/*.md\" \"!**/node_modules/**/*.md\" README.md",
