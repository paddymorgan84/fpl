{
  "name": "fpl",
  "version": "1.0.0",
  "description": "A Go tool for Fantasy Premier League",
  "main": "index.js",
  "dependencies": {
    "doctoc": "^2.2.1",
    "markdown-spellcheck": "^1.3.1",
